{
  "name": "opticks",
  "version": "4.0.3",
  "description": "FindHotel Toggle Flag JavaScript SDK",
  "main": "lib/optimizely.ts",
  "module": "lib/optimizely.mjs",
  "types": "lib/optimizely.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "clean:booleanToggle": "jscodeshift --transform lib/transform/booleanToggle.js",
    "clean:toggle": "jscodeshift --transform lib/transform/toggle.js",
    "jscodeshift": "jscodeshift",
    "prepare": "npm run build",
    "release": "npm run prepare && npx changeset publish",
    "test": "jest src",
    "test:watch": "jest --watch src",
    "changeset": "npx changeset"
  },
  "keywords": [
    "experiment",
    "ab testing",
    "feature-flag",
    "toggle",
    "flag",
    "canary",
    "codemod",
    "jscodeshift"
  ],
  "engines": {
    "node": "16.13.1"
  },
  "author": "Jop de Klein",
  "license": "ISC",
  "peerDependencies": {
    "@optimizely/optimizely-sdk": "4.2.1",
    "jscodeshift": "^0.14.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@optimizely/optimizely-sdk": "4.2.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.0",
    "jscodeshift": "^0.14.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
