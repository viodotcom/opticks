{
  "name": "opticks",
  "version": "4.0.0",
  "description": "FindHotel Toggle Flag JavaScript SDK",
  "main": "lib/simple.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean:booleanToggle": "jscodeshift --parser flow --transform lib/transform/booleanToggle.js",
    "clean:toggle": "jscodeshift --parser flow --transform lib/transform/toggle.js",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "jscodeshift": "jscodeshift",
    "prettier-eslint": "prettier-eslint",
    "prepare": "npm run prepare:flow && npm run build",
    "prepare:flow": "flow-copy-source src lib -i test.js",
    "test": "jest src",
    "test:watch": "jest --watch src"
  },
  "keywords": [
    "experiment",
    "ab testing",
    "feature-flag",
    "toggle",
    "flag",
    "canary",
    "codemod",
    "jscodeshift"
  ],
  "author": "Jop de Klein",
  "license": "ISC",
  "peerDependencies": {
    "@optimizely/optimizely-sdk": "4.2.1",
    "jscodeshift": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@optimizely/optimizely-sdk": "4.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^23.6.0",
    "jscodeshift": "^0.5.1",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^3.0.0",
    "xo": "^0.51.0"
  }
}