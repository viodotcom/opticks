{
  "name": "opticks",
  "version": "2.0.0",
  "description": "FindHotel Toggle Flag JavaScript SDK",
  "main": "lib/simple.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean:booleanToggle": "jscodeshift --parser flow --transform lib/transform/booleanToggle.js",
    "clean:toggle": "jscodeshift --parser flow --transform lib/transform/toggle.js",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "jscodeshift": "jscodeshift",
    "prettier-eslint": "prettier-eslint",
    "prepare": "npm run prepare:flow && npm run build",
    "prepare:flow": "flow-copy-source src lib -i test.js",
    "test": "jest src",
    "test:watch": "jest --watch src"
  },
  "keywords": [
    "experiment",
    "ab testing",
    "feature-flag",
    "toggle",
    "flag",
    "canary",
    "codemod",
    "jscodeshift"
  ],
  "author": "Jop de Klein",
  "license": "ISC",
  "peerDependencies": {
    "@optimizely/optimizely-sdk": "^3.0.1",
    "jscodeshift": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@optimizely/optimizely-sdk": "^3.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.83.0",
    "flow-copy-source": "^2.0.2",
    "flow-typed": "^2.5.1",
    "jest": "^23.6.0",
    "jscodeshift": "^0.5.1",
    "prettier-eslint-cli": "^4.7.1",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^3.0.0"
  }
}
